{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_NEON_AUTH_URL": "https://ep-morning-shadow-a1p4v4nw.neonauth.ap-southeast-1.aws.neon.tech/neondb/auth"
      }
    },
    "production": {
      "environment": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "production_store": {
      "environment": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
