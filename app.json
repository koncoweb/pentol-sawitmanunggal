{
  "expo": {
    "name": "PENTOL - Pencatatan Online",
    "slug": "pentol-harvest-management",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/lg-aep-cmyk-300dpi.jpg",
    "splash": {
      "image": "./assets/images/lg-aep-cmyk-300dpi copy.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "pentol",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sawitmanunggal.pentol",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lg-aep-cmyk-300dpi.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "79e9f14f-8c9a-4111-b59c-a607cc8c8d88"
      }
    },
    "owner": "koncoweb"
  }
}
